    "import csv\n",
    "import json\n",
    "\n",
    "csvfile = open('Adults with Asthma 2014.csv', 'r')\n",
    "jsonfile = open('data.json', 'w')\n",
    "\n",
    "fieldnames = (\"Borough\",\"Geography\",\"ID\",\"Number\")\n",
    "reader = csv.DictReader(csvfile, fieldnames)\n",
    "out = json.dumps( [ row for row in reader ] )\n",
    "jsonfile.write(out)"
   ]
